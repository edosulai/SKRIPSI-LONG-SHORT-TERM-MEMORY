{
  "name": "jstools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "concurrently \"npm:django\" \"npm:tailwind-minify\" \"npm:webpack-minify\"",
    "dev": "concurrently \"npm:django\" \"npm:tailwind-watch\" \"npm:webpack-watch\"",
    "django": "run-script-os",
    "django:darwin:linux": "./manage.py runserver 80",
    "django:win32": ".\\manage.py runserver 80",
    "freeze": "pip freeze > requirements.txt",
    "update": "pip install -r requirements.txt",
    "tailwind-watch": "set TAILWIND_MODE=watch && tailwind -i resources/css/style.css -o static/css/app.css --watch",
    "tailwind-minify": "set TAILWIND_MODE=minify && tailwind -i resources/css/style.css -o static/css/app.css --minify",
    "webpack-watch": "set WEBPACK_MODE=watch && webpack --watch",
    "webpack-minify": "set WEBPACK_MODE=minify && webpack --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "simple-datatables": "^3.2.0",
    "tailwindcss": "^3.0.18",
    "vue": "^3.2.30",
    "vue3-click-away": "^1.2.1",
    "webpack": "^5.68.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "run-script-os": "^1.1.6",
    "webpack-cli": "^4.9.2"
  }
}
