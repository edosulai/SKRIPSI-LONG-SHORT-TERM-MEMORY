{
  "name": "jstools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "concurrently \"npm:django\" \"npm:tailwind-minify\" \"npm:webpack-minify\"",
    "dev": "concurrently \"npm:django\" \"npm:tailwind-watch\" \"npm:webpack-watch\"",
    "django": "run-script-os",
    "django:darwin:linux": "./manage.py runserver 80",
    "django:win32": ".\\manage.py runserver 80",
    "tailwind-watch": "tailwind -i resources/css/style.css -o static/css/app.css --watch",
    "tailwind-minify": "tailwind -i resources/css/style.css -o static/css/app.css --minify",
    "webpack-watch": "webpack watch --mode development",
    "webpack-minify": "webpack build",
    "seed": "run-script-os",
    "seed:darwin:linux": "./manage.py shell -c 'exec(open(\"dumpdata.py\").read())'",
    "seed:win32": ".\\manage.py shell -c 'exec(open(\"dumpdata.py\").read())'",
    "env": "run-script-os",
    "env:darwin:linux": "./env/Scripts/activate",
    "env:win32": ".\\env\\Scripts\\activate.ps1"
  },
  "note": [
    "pip freeze > requirements.txt",
    "pip install -r requirements.txt",
    "py -m venv env",
    "py manage.py createsuperuser",
    "py manage.py makemigrations",
    "py manage.py migrate"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@kurkle/color": "^0.1.9",
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "chart.js": "^3.7.0",
    "chartjs-adapter-luxon": "^1.1.0",
    "luxon": "^2.3.0",
    "simple-datatables": "^3.2.0",
    "tailwindcss": "^3.0.18",
    "vue": "^3.2.30",
    "vue3-click-away": "^1.2.1",
    "webpack": "^5.68.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "run-script-os": "^1.1.6",
    "webpack-cli": "^4.9.2"
  }
}
